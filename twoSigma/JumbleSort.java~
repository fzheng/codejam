import java.io.*;

public class JumbleSort {
    public static void main(String[] args) {
        byte data[] = new byte[10];
        System.out.println("Please input some bytes: ");
        System.in.read(data);
        System.out.println("You entered: ");
        for (byte b : data) {
            System.out.print(b);
        }
    }
}